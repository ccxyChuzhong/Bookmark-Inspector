{
  "manifest_version": 3,
  "name": "书签管理器",
  "version": "1.0.1",
  "description": "一款支持导入导出和验证书签的工具",
  "permissions": [
    "bookmarks",
    "downloads",
    "storage",
    "windows"
  ],
  "action": {
    "default_title": "书签管理器",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png",
      "256": "images/icon256.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png",
    "256": "images/icon256.png"
  },
  "default_locale": "en",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; style-src 'self' 'unsafe-inline'; font-src 'self';"
  }
}